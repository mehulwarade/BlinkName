int led2 = D7;

void setup() {
  pinMode(led2, OUTPUT);

}


void loop() {
//Showing "M"
digitalWrite(led2, HIGH);
  delay(1000); //Long Blink
  digitalWrite(led2, LOW);
  delay(1000);
  
  digitalWrite(led2, HIGH);
  delay(1000); //Long Blink
  digitalWrite(led2, LOW);
  delay(1000);
//Showing "E"
    digitalWrite(led2, HIGH);
  delay(500); //Short blink
  digitalWrite(led2, LOW);
  delay(1000);
//Showing "H"
digitalWrite(led2, HIGH);
  delay(500); //Short blink
  digitalWrite(led2, LOW);
  delay(1000);
  
  digitalWrite(led2, HIGH);
  delay(500); //Short blink
  digitalWrite(led2, LOW);
  delay(1000);
  
  digitalWrite(led2, HIGH);
  delay(500); //Short blink
  digitalWrite(led2, LOW);
  delay(1000);
  
  digitalWrite(led2, HIGH);
  delay(500); //Short blink
  digitalWrite(led2, LOW);
  delay(1000);
//Showing "U"
    digitalWrite(led2, HIGH);
  delay(500); //Short blink
  digitalWrite(led2, LOW);
  delay(1000);
  
  digitalWrite(led2, HIGH);
  delay(500); //Short blink
  digitalWrite(led2, LOW);
  delay(1000);
  
  digitalWrite(led2, HIGH);
  delay(1000); //Long Blink
  digitalWrite(led2, LOW);
  delay(1000);
//Showing "L"
    digitalWrite(led2, HIGH);
  delay(500); //Short blink
  digitalWrite(led2, LOW);
  delay(1000);

  digitalWrite(led2, HIGH);
  delay(1000); //Long Blink
  digitalWrite(led2, LOW);
  delay(1000);

    digitalWrite(led2, HIGH);
  delay(500); //Short blink
  digitalWrite(led2, LOW);
  delay(1000);


  digitalWrite(led2, HIGH);
  delay(500); //Short blink
  digitalWrite(led2, LOW);
  delay(1000);
}
